 tell application "Terminal"
	activate		activate
	tell application "System Events"		tell application "System Events"
		keystroke "t" using command down # new tab			keystroke "t" using command down # new tab
		keystroke "~/Desktop/NervaWallet/nervad"			keystroke "sudo ~/Desktop/NervaWallet/nervad"
		key code 36 # press enter			key code 36 # press enter
		keystroke "t" using command down # new tab			keystroke "t" using command down # new tab
		keystroke "~/Desktop/NervaWallet/nerva-wallet-cli"			keystroke "sudo ~/Desktop/NervaWallet/nerva-wallet-cli"
		key code 36 # press enter			key code 36 # press enter
	end tell		end tell
end tell	end tell
